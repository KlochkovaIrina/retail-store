                           <div class="filter-content__section">
                              <div class="filter-content__title title"><span></span>Размер</div>
                              <div class="filter-content__body">
                                 <label class="filter-content__checkbox checkbox">
                                    <input data-error="Ошибка" class="checkbox__input" type="checkbox" value="1"
                                       name="form[]">
                                    <span class="checkbox__text"><span>40</span></span>
                                    <div class="checkbox__count">
                                       15
                                    </div>
                                 </label>
                                 <label class="filter-content__checkbox checkbox">
                                    <input data-error="Ошибка" class="checkbox__input" type="checkbox" value="1"
                                       name="form[]">
                                    <span class="checkbox__text"><span>42</span></span>
                                    <div class="checkbox__count">
                                       15
                                    </div>
                                 </label>
                                 <label class="filter-content__checkbox checkbox">
                                    <input data-error="Ошибка" class="checkbox__input" type="checkbox" value="1"
                                       name="form[]">
                                    <span class="checkbox__text"><span>44</span></span>
                                    <div class="checkbox__count">
                                       15
                                    </div>
                                 </label>
                                 <label class="filter-content__checkbox checkbox">
                                    <input data-error="Ошибка" class="checkbox__input" type="checkbox" value="1"
                                       name="form[]">
                                    <span class="checkbox__text"><span>46</span></span>
                                    <div class="checkbox__count">
                                       15
                                    </div>
                                 </label>
                                 <label class="filter-content__checkbox checkbox">
                                    <input data-error="Ошибка" class="checkbox__input" type="checkbox" value="1"
                                       name="form[]">
                                    <span class="checkbox__text"><span>48</span></span>
                                    <div class="checkbox__count">
                                       15
                                    </div>
                                 </label>

                              </div>
                           </div>
                        </div>
                     </form>
                  </div>
               </aside>
               <div class="content__page"></div>
            </div>
         </div>
      </main>
      <!-- @import "footer.html" -->
   </div>



   <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
   <script src="https://maps.google.com/maps/api/js?sensor=false&amp;key="></script>
   <!-- подключение карты  -->
   <script src="js/app.js"></script>

</body>

</html>